/* Permutation. Given two integer arrays of size n, design a algorithm to determine whether one is a 
  permutation of the other. That is, do they contain exactly the same entries but, possibly, in a different order. */
  
#include<stdio.h>
#include<stdlib.h>
void sort(int array[40], int n)
{
  int i,j,n,temp;
  for(i = 0 ; i < n - 1 ; i++)
     for(j = i ; j < n ;j++)
        if(a[i] > a[j])
        {
          temp = a[i] ;
          a[i] = a[j];
          a[j] = temp;
          }
 }
 void main()
 {
  int a[40], b[40], i, n;
  scanf("%d ", &n);
  for(i = 0 ; i < n; i++)
    scanf("%d", &a[i]);
  for(i = 0 ; i < n; i++)
    scanf("%d", &b[i]);
 for(i = 0 ; i < n; i++)
    if(a[i] != b[i])
    {
      printf("No\n");
      exit(0);
     }
 printf("Yes\n");
  
  
